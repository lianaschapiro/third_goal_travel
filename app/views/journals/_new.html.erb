<%= form_for [@country, @journal] do |j| %>
	<%= j.label :title %>
	<%= j.text_field :title %>
	<%= j.label :description %>
	<%= j.text_field :description %>
	<br>
	<%= j.label :link %>
	<%= j.text_field :link %>
	<%= j.label :author %>
	<%= j.text_field :author %>
	<%= j.submit value: "Add" %>
<% end %>